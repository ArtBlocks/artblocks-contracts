{
  "name": "@artblocks/sdk",
  "version": "0.1.30-13",
  "description": "JavaScript SDK for configuring and using Art Blocks minters.",
  "main": "dist/index.js",
  "repository": "git@github.com:ArtBlocks/artblocks-sdk.git",
  "author": "Art Blocks, Inc. <privacy@artblocks.io>",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./dist/machines/*": {
      "types": "./dist/machines/*/index.d.ts",
      "default": "./dist/machines/*/index.js"
    },
    "./machines/*": {
      "types": "./dist/machines/*/index.d.ts",
      "default": "./dist/machines/*/index.js"
    },
    "./machines/utils": {
      "types": "./dist/machines/utils.d.ts",
      "default": "./dist/machines/utils.js"
    },
    "./dist/machines/utils": {
      "types": "./dist/machines/utils.d.ts",
      "default": "./dist/machines/utils.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@types/lodash": "^4.17.5",
    "abitype": "^1.0.6",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.3",
    "jsdoc-plugin-typescript": "^2.2.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.2",
    "tsconfig": "*",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "yarn codegen && tsup src/index.ts src/machines/**/index.ts src/machines/utils.ts --dts",
    "codegen": "yarn --cwd=\"../..\" run codegen:project sdk",
    "lint": "prettier --check src/**/*.ts && npx eslint src/**/*.ts",
    "fmt": "prettier --write src/**/*.ts && npx eslint src/**/*.ts --fix",
    "test": "jest",
    "build:docs": "jsdoc -c jsdoc.json",
    "pre-commit": "yarn lint",
    "pre-push": "yarn codegen && yarn test"
  },
  "lint-staged": {
    "*.{js,ts,tsx, jsx}": [
      "npx eslint --quiet --fix"
    ],
    "*.{json,md,html,js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/generated"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "dependencies": {
    "@artblocks/contracts": "^1.0.2",
    "encoding": "^0.1.13",
    "graphql": "^16.8.2",
    "graphql-request": "^7.1.1",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.3.11",
    "viem": "^2.13.7",
    "xstate": "5.13.2",
    "zod": "^3.23.8"
  },
  "files": [
    "dist"
  ]
}
